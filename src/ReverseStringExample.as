package{	import flash.display.*;	import flash.events.*;	import com.tribalframework.utils.reverseString.ReverseString;		public class ReverseStringExample extends MovieClip	{		public function ReverseStringExample(){			btn.addEventListener(MouseEvent.CLICK, go);		}				private function go(e:MouseEvent){			var rs = new ReverseString();			var str = input_txt.text;			output_txt.text = rs.reverseIt(str);		}	}}