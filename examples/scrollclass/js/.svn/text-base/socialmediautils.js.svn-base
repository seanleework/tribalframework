function fbs_click() {window.open('http://www.facebook.com/sharer.php?u=' + encodeURIComponent(document.location.href) + '&t=' + encodeURIComponent(document.title), 'sharer', 'toolbar=0,status=0,width=626,height=436');return false;}
function tws_click() {window.open('http://twitter.com/home?status='+encodeURIComponent(document.title)+'+'+encodeURIComponent(document.location.href), 'sharer');return false;};