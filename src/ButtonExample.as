package{	import flash.display.*;	import flash.events.*;	import com.tribalframework.ui.button.ButtonClass;		public class ButtonExample extends ButtonClass	{		private var btnArray:Array;		private var btnn:btn;				public function ButtonExample(){					//	btnArray = [mc0, mc1];			btnArray = new Array();			var tempX = 0;						for(var p = 0; p < 5; p++){				btnn = new btn();				btnn.name = "mc"+p;				btnn.x = tempX;				tempX = btnn.x + btnn.width + 10;				addChild(btnn);				btnArray.push(btnn);								setBtn(btnArray[p]);				}					}				override public function clickEvent(e:Event){						if(e.target.name == "mc2"){				trace('yay');			}						for(var i = 0; i < btnArray.length; i++){				if(e.target.name.split("mc").join("") == i){					btnArray[i].selected = true;					btnArray[i].gotoAndStop("onClick");					btnArray[i].mouseEnabled = false;				}				else{					btnArray[i].selected = false;					btnArray[i].gotoAndStop("out");					btnArray[i].mouseEnabled = true;				}			}					}			}	}