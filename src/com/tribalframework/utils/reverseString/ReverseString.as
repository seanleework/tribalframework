package com.tribalframework.utils.reverseString{	import flash.display.*;		public class ReverseString extends Sprite	{		private var arr:Array;				public function ReverseString(){					}				public function reverseIt(str:String):String{			arr = new Array();			arr = str.split("");			var p = String(arr.reverse());			var q = p.split(",").join("");						return q;		}	}}